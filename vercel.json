{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/"
        },
        {
            "src": "/api/test",
            "dest": "/api/test"
        },
        {
            "src": "/api/user/signup",
            "dest": "/api/user/signup"
        },
        {
            "src": "/api/user/login",
            "dest": "/api/user/login"
        },
        {
            "src": "/api/salesuser/signup",
            "dest": "/api/salesuser/signup"
        },
        {
            "src": "/api/salesuser/login",
            "dest": "/api/salesuser/login"
        }
    ]
}