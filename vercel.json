{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/"
        },
        {
            "src": "/api/test",
            "dest": "/api/test"
        },
        {
            "src": "/api/user/signup",
            "dest": "/api/user/signup"
        },
        {
            "src": "/api/user/login",
            "dest": "/api/user/login"
        },
        {
            "src": "/api/salesuser/signup",
            "dest": "/api/salesuser/signup"
        },
        {
            "src": "/api/salesuser/login",
            "dest": "/api/salesuser/login"
        },
        {
            "src": "/api/productionUser/signup",
            "dest": "/api/productionUser/signup"
        },
        {
            "src": "/api/productionUser/login",
            "dest": "/api/productionUser/login"
        },
        {
            "src": "/api/employee/submit",
            "dest": "/api/employee/submit"
        },
        {
            "src": "/api/employee/fetch",
            "dest": "/api/employee/fetch"
        },
        {
            "src": "/api/employee/search/:id",
            "dest": "/api/employee/search/:id"
        },
        {
            "src": "/api/employee/addRecord/:id",
            "dest": "/api/employee/addRecord/:id"
        },
        {
            "src": "/api/dropdown/projectNature",
            "dest": "/api/dropdown/projectNature"
        },
        {
            "src": "/api/dropdown/platform",
            "dest": "/api/dropdown/platform"
        },
        {
            "src": "/api/dropdown/department",
            "dest": "/api/dropdown/department"
        },
        {
            "src": "/api/dropdown/getProjectNature",
            "dest": "/api/dropdown/getProjectNature"
        },
        {
            "src": "/api/dropdown/getPlatform",
            "dest": "/api/dropdown/getPlatform"
        },
        {
            "src": "/api/dropdown/getDepartment",
            "dest": "/api/dropdown/getDepartment"
        },
        {
            "src": "/api/projects/submit",
            "dest": "/api/projects/submit"
        },
        {
            "src": "/api/projects/get",
            "dest": "/api/projects/get"
        }
    ]
}